<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="mhs.team.googlemapsapp.Recycle"
    android:id="@+id/activity_icon_page"
    android:background="#2ecc71">

    <ImageButton
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:id="@+id/paperButton"
        android:background="@mipmap/flatpaper"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:id="@+id/plasticButton"
        android:background="@mipmap/flatplastic"
        android:layout_above="@+id/cardboardButton"
        android:layout_alignLeft="@+id/metalButton"
        android:layout_alignStart="@+id/metalButton" />

    <ImageButton
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:id="@+id/cardboardButton"
        android:background="@mipmap/flatcardboard"
        android:layout_below="@+id/paperButton"
        android:layout_alignLeft="@+id/glassButton"
        android:layout_alignStart="@+id/glassButton"
        android:layout_marginTop="40dp" />

    <ImageButton
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:id="@+id/metalButton"
        android:background="@mipmap/flatmetal"
        android:layout_alignTop="@+id/cardboardButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="28dp"
        android:layout_marginEnd="28dp" />

    <ImageButton
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:id="@+id/glassButton"
        android:background="@mipmap/flatglass"
        android:layout_above="@+id/textView6"
        android:layout_alignLeft="@+id/paperButton"
        android:layout_alignStart="@+id/paperButton"
        android:layout_marginBottom="10dp" />

    <ImageButton
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:id="@+id/woodButton"
        android:background="@mipmap/flatwood"
        android:layout_alignTop="@+id/glassButton"
        android:layout_alignRight="@+id/metalButton"
        android:layout_alignEnd="@+id/metalButton"
        android:layout_alignLeft="@+id/metalButton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Paper"
        android:id="@+id/textView2"
        android:textColor="#010101"
        android:layout_marginTop="9dp"
        android:layout_below="@+id/paperButton"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Plastic"
        android:id="@+id/textView3"
        android:textColor="#010101"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cardboard"
        android:id="@+id/textView4"
        android:textColor="#010101"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignLeft="@+id/cardboardButton"
        android:layout_alignStart="@+id/cardboardButton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Metal Cans"
        android:id="@+id/textView5"
        android:textColor="#010101"
        android:layout_marginTop="10dp"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp"
        android:layout_below="@+id/metalButton"
        android:layout_alignRight="@+id/metalButton"
        android:layout_alignEnd="@+id/metalButton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Glass"
        android:id="@+id/textView6"
        android:textColor="#010101"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wood"
        android:id="@+id/textView7"
        android:textColor="#010101"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView3"
        android:layout_alignEnd="@+id/textView3" />
</RelativeLayout>
