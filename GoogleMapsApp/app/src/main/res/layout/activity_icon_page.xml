<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="mhs.team.googlemapsapp.Recycle"
    android:id="@+id/activity_icon_page"
    android:background="#2980b9">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/linearLayout2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical" >

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

        </LinearLayout>
</LinearLayout>

    <ImageButton
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_110sdp"
        android:id="@+id/paperButton"
        android:background="@mipmap/flatpaper"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_above="@+id/cardboardButton"
        android:layout_toRightOf="@+id/linearLayout2"
        android:layout_toEndOf="@+id/linearLayout2"
        android:layout_marginBottom="27dp" />

    <ImageButton
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_110sdp"
        android:id="@+id/plasticButton"
        android:background="@mipmap/flatplastic"
        android:layout_alignTop="@+id/paperButton"
        android:layout_alignLeft="@+id/metalButton"
        android:layout_alignStart="@+id/metalButton" />

    <ImageButton
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_110sdp"
        android:id="@+id/metalButton"
        android:background="@mipmap/flatmetal"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp"
        android:layout_alignTop="@+id/cardboardButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_110sdp"
        android:id="@+id/woodButton"
        android:background="@mipmap/flatwood"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/metalButton"
        android:layout_alignStart="@+id/metalButton"
        android:layout_marginBottom="33dp" />

    <ImageButton
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_110sdp"
        android:id="@+id/glassButton"
        android:background="@mipmap/flatglass"
        android:layout_alignTop="@+id/woodButton"
        android:layout_alignLeft="@+id/cardboardButton"
        android:layout_alignStart="@+id/cardboardButton" />

    <ImageButton
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_110sdp"
        android:id="@+id/cardboardButton"
        android:background="@mipmap/flatcardboard"
        android:layout_above="@+id/glassButton"
        android:layout_alignLeft="@+id/paperButton"
        android:layout_alignStart="@+id/paperButton"
        android:layout_marginBottom="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/textView6"
        android:layout_alignBottom="@+id/linearLayout2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Choose a Material"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:textColor="#ecf0f1"
        android:textSize="@dimen/_25sdp" />

</RelativeLayout>
